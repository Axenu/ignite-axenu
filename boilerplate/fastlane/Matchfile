git_url("<%= props.git_secrets %>")

storage_mode("git")

type("development") # The default type, can be: appstore, adhoc, enterprise or development

app_identifier(["<%= props.app_id %>"])
username("<%= props.email %>") # Your Apple Developer Portal username

